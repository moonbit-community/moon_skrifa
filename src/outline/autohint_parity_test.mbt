///|
/// Autohint parity tests against `fontations-reference/skrifa`.
///
/// Expected SVG paths are generated with:
/// `cargo run -q --manifest-path fontations-reference/Cargo.toml -p moon_skrifa_refdump --bin moon_skrifa_refdump -- <font> <gid> <ppem> auto ft 3`

///|
fn autohint_parity_font_notoserif_autohint_shaping_b64() -> String {
  ""
}

///|
fn autohint_parity_font_notoserifhebrew_autohint_metrics_b64() -> String {
  ""
}

///|
#skip
test "autohint: notoserif_autohint_shaping gid=1 size=16 ft" {
  let font = golden_font(autohint_parity_font_notoserif_autohint_shaping_b64())
  let outlines0 = OutlineGlyphCollection::from_font(font)
  let h = HintingInstance::new(
    outlines0,
    @moon_skrifa.Size::new(16.0),
    @moon_skrifa.LocationRef::default(),
    HintingOptions::new(Engine::Auto(None), Target::default()),
  ).unwrap()
  let outlines = OutlineGlyphCollection::from_font(font)
  let glyph = outlines.get(@moon_skrifa.GlyphId::new((1).to_uint16())).unwrap()
  let settings = DrawSettings::hinted(h, false).with_path_style(
    PathStyle::FreeType,
  )
  let pen = SvgPen::with_precision(3)
  glyph.draw(settings, pen).unwrap() |> ignore
  inspect(
    pen.to_string(),
    content="M0.484,0.000 L0.484,0.703 L0.688,0.703 Q1.234,0.703 1.609,0.906 Q2.000,1.125 2.000,1.906 L2.000,10.156 Q2.000,10.891 1.609,11.094 Q1.219,11.297 0.688,11.297 L0.484,11.297 L0.484,12.000 L5.203,12.000 L5.203,11.297 L4.984,11.297 Q4.422,11.297 4.016,11.094 Q3.625,10.891 3.625,10.156 L3.625,6.922 L9.000,6.922 L9.000,10.156 Q9.000,10.891 8.594,11.094 Q8.203,11.297 7.641,11.297 L7.422,11.297 L7.422,12.000 L12.141,12.000 L12.141,11.297 L11.938,11.297 Q11.391,11.297 11.000,11.078 Q10.625,10.875 10.625,10.094 L10.625,1.828 Q10.625,1.094 11.016,0.891 Q11.406,0.703 11.938,0.703 L12.141,0.703 L12.141,0.000 L7.422,0.000 L7.422,0.703 L7.641,0.703 Q8.203,0.703 8.594,0.922 Q9.000,1.141 9.000,1.969 L9.000,6.078 L3.625,6.078 L3.625,1.969 Q3.625,1.141 4.016,0.922 Q4.422,0.703 4.984,0.703 L5.203,0.703 L5.203,0.000 Z",
  )
}

