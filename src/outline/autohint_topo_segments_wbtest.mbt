///|
/// Autohint topology parity tests (segments) against fontations-reference.
///
/// Ported from `fontations/skrifa/src/outline/autohint/topo/segments.rs` tests
/// (Apache-2.0 OR MIT).

///|
fn b64_val(c : Int) -> Int? {
  if c >= 65 && c <= 90 {
    Some(c - 65)
  } else if c >= 97 && c <= 122 {
    Some(c - 97 + 26)
  } else if c >= 48 && c <= 57 {
    Some(c - 48 + 52)
  } else if c == 43 {
    Some(62)
  } else if c == 47 {
    Some(63)
  } else {
    None
  }
}

///|
fn b64_decode(s : String) -> Bytes {
  let out : Array[Byte] = Array::new()
  let vals : Array[Int] = Array::new()
  let n = s.length()
  for i in 0..<n {
    let c = s.code_unit_at(i).to_int()
    if c == 61 {
      // "=" padding
      vals.push(-1)
    } else {
      match b64_val(c) {
        None => ()
        Some(v) => vals.push(v)
      }
    }
    if vals.length() == 4 {
      let v0 = vals.at(0)
      let v1 = vals.at(1)
      let v2 = vals.at(2)
      let v3 = vals.at(3)
      out.push((((v0 << 2) | (v1 >> 4)) & 0xFF).to_byte())
      if v2 != -1 {
        out.push(((((v1 & 0x0F) << 4) | (v2 >> 2)) & 0xFF).to_byte())
        if v3 != -1 {
          out.push(((((v2 & 0x03) << 6) | v3) & 0xFF).to_byte())
        }
      }
      vals.clear()
    }
  }
  Bytes::from_array(out.op_as_view())
}

///|
fn golden_font(b64 : String) -> @moon_skrifa.FontRef {
  @moon_skrifa.FontRef::new(b64_decode(b64)).unwrap()
}

///|
fn autohint_segments_test_view(
  segments : Array[AutoHintSegment],
) -> Array[AutoHintSegment] {
  let out : Array[AutoHintSegment] = Array::new()
  for s in segments.iter() {
    let t = AutoHintSegment::default()
    t.flags = s.flags
    t.dir = s.dir
    t.pos = s.pos
    t.delta = s.delta
    t.min_coord = s.min_coord
    t.max_coord = s.max_coord
    t.height = s.height
    t.link_ix = s.link_ix
    t.serif_ix = s.serif_ix
    out.push(t)
  }
  out
}

///|
fn autohint_expected_segment(
  flags : Int,
  dir : AutoHintDirection,
  pos : Int,
  delta : Int,
  min_coord : Int,
  max_coord : Int,
  height : Int,
  link_ix : Int?,
  serif_ix : Int?,
) -> AutoHintSegment {
  let s = AutoHintSegment::default()
  s.flags = flags
  s.dir = dir
  s.pos = pos
  s.delta = delta
  s.min_coord = min_coord
  s.max_coord = max_coord
  s.height = height
  s.link_ix = link_ix
  s.serif_ix = serif_ix
  s
}

///|
fn autohint_dir_debug(dir : AutoHintDirection) -> String {
  match dir {
    AutoHintDirection::None_ => "None"
    AutoHintDirection::Right => "Right"
    AutoHintDirection::Left => "Left"
    AutoHintDirection::Up => "Up"
    AutoHintDirection::Down => "Down"
  }
}

///|
fn autohint_opt_int_debug(v : Int?) -> String {
  match v {
    None => "None"
    Some(n) => n.to_string()
  }
}

///|
fn autohint_segments_debug(segments : Array[AutoHintSegment]) -> String {
  let mut s = "["
  for i in 0..<segments.length() {
    let seg = segments.at(i)
    if i != 0 {
      s = s + ", "
    }
    s = s +
      "{flags=" +
      seg.flags.to_string() +
      ",dir=" +
      autohint_dir_debug(seg.dir) +
      ",pos=" +
      seg.pos.to_string() +
      ",delta=" +
      seg.delta.to_string() +
      ",min=" +
      seg.min_coord.to_string() +
      ",max=" +
      seg.max_coord.to_string() +
      ",height=" +
      seg.height.to_string() +
      ",link=" +
      autohint_opt_int_debug(seg.link_ix) +
      ",serif=" +
      autohint_opt_int_debug(seg.serif_ix) +
      "}"
  }
  s + "]"
}

///|
test "autohint topo: segments notoserifhebrew gid=8 horizontal" {
  autohint_parity_font_notoserifhebrew_autohint_metrics_b64() |> ignore
  let font = golden_font(notoserifhebrew_autohint_metrics_b64())
  let gid = @moon_skrifa.GlyphId::new((8).to_uint16())
  let outline = AutoHintOutline::from_path(Array::new())
  match outline._fill_unscaled(font, gid) {
    Ok(_) => ()
    Err(e) => {
      let kind = match e {
        DrawError::NoSources => "NoSources"
        DrawError::GlyphNotFound(_) => "GlyphNotFound"
        DrawError::InsufficientMemory => "InsufficientMemory"
        DrawError::RecursionLimitExceeded(_) => "RecursionLimitExceeded"
        DrawError::TooManyPoints(_) => "TooManyPoints"
        DrawError::HintingFailed(_) => "HintingFailed"
        DrawError::InvalidAnchorPoint(_, _) => "InvalidAnchorPoint"
        DrawError::PostScript => "PostScript"
        DrawError::ToPath(_) => "ToPath"
        DrawError::Read => "Read"
        DrawError::HarfBuzzHintingUnsupported => "HarfBuzzHintingUnsupported"
      }
      inspect(kind, content="Ok")
    }
  }
  let axis = AutoHintAxis::default()
  axis.reset(AUTOHINT_DIM_HORIZONTAL, outline.orientation)
  inspect(
    autohint_compute_segments(outline, axis, AutoHintScriptGroup::Default),
    content="true",
  )
  autohint_link_segments(outline, axis, 0, AutoHintScriptGroup::Default, None)
  let got = autohint_segments_test_view(axis.segments)
  let expected : Array[AutoHintSegment] = Array::new()
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Up,
      55,
      0,
      26,
      360,
      372,
      Some(3),
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Up,
      112,
      0,
      481,
      504,
      34,
      Some(2),
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Down,
      168,
      0,
      483,
      504,
      26,
      Some(1),
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Down,
      109,
      0,
      109,
      366,
      288,
      Some(0),
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Up,
      453,
      0,
      169,
      432,
      304,
      Some(7),
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      1,
      AutoHintDirection::Up,
      62,
      0,
      517,
      566,
      76,
      None,
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      1,
      AutoHintDirection::Down,
      103,
      0,
      619,
      647,
      41,
      None,
      None,
    ),
  )
  expected.push(
    autohint_expected_segment(
      0,
      AutoHintDirection::Down,
      507,
      0,
      40,
      485,
      498,
      Some(4),
      None,
    ),
  )
  inspect(
    autohint_segments_debug(got),
    content=autohint_segments_debug(expected),
  )
}

